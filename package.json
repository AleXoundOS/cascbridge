{
  "name": "vite-webpack-electron",
  "version": "0.0.1",
  "main": "dist/main/index.js",
  "author": "草鞋没号 <308487730@qq.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.17.0"
  },
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "package": "rimraf dist && npm run build && electron-builder"
  },
  "build": {
    "appId": "308487730@qq.com",
    "asar": false,
    "directories": {
      "output": "release/${version}"
    },
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    },
    "files": [
      "!node_modules",
      "dist/**"
    ],
    "mac": {
      "artifactName": "${productName}_${version}.${ext}",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}_${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    }
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^2.2.8",
    "vue": "^3.2.23",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@vitejs/plugin-vue": "^1.10.1",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "chalk": "^4.1.0",
    "electron": "16.0.2",
    "electron-builder": "^22.14.5",
    "electron-store": "^8.0.1",
    "i": "^0.3.7",
    "less": "^4.1.2",
    "minimist": "^1.2.5",
    "npm": "^8.3.0",
    "ora": "^6.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.3",
    "vite": "^2.7.1",
    "webpack": "^5.65.0",
    "webpack-merge": "^5.8.0"
  },
  "keywords": [
    "vite",
    "electron",
    "vue3",
    "webpack"
  ]
}
