{
  "name": "vite-webpack-electron",
  "version": "0.0.1",
  "main": "dist/main/index.js",
  "author": "草鞋没号 <308487730@qq.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.17.0"
  },
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "rimraf dist && node scripts/build.js && electron-builder"
  },
  "build": {
    "appId": "308487730@qq.com",
    "asar": false,
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "!node_modules",
      "dist/**"
    ],
    "mac": {
      "artifactName": "${productName}_${version}.${ext}",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}_${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    }
  },
  "dependencies": {
    "antd": "^4.17.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.1.3",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "chalk": "^4.1.0",
    "electron": "16.0.2",
    "electron-builder": "^22.14.5",
    "electron-store": "^8.0.1",
    "less": "^4.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.3",
    "vite": "^2.7.1",
    "vite-plugin-style-import": "^1.4.0",
    "vitejs-plugin-electron": "^1.0.3",
    "webpack": "^5.65.0",
    "webpack-merge": "^5.8.0"
  },
  "env": {
    "//": "Used in build scripts",
    "HOST": "127.0.0.1",
    "PORT": 5555
  },
  "keywords": [
    "vite",
    "electron",
    "react",
    "webpack"
  ]
}
